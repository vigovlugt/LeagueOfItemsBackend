{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "path": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "Github": {
    "Owner": "vigovlugt",
    "Repository": "LeagueOfItemsFrontend",
    "Path": "data/dataset.json"
  },
  "Ugg": {
    "ApiUrl": "https://stats2.u.gg/",
    "PatchPageUrl": "https://u.gg/lol/tier-list/"
  },
  "Riot": {
    "ApiUrl": "https://ddragon.leagueoflegends.com/"
  }
}
